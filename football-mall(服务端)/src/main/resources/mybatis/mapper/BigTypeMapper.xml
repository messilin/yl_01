<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.store.footballstore.mapper.BigTypeMapper">
<!--根据Id查询商品大类-->
    <resultMap id="bigTypeResult" type="com.store.footballstore.entity.BigType"></resultMap>
    <select id="findById" parameterType="Integer" resultMap="bigTypeResult">
        select * from table_bigtype where id=#{id}
    </select>
</mapper>